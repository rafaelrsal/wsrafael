package variaveis;

public class TiposDados {

	//Comentário é com //
	
	/*
	 * bloco de comentarios 
	 */	
	public static void main(String[] args) {
		
		String nome = "Rafael";
		int idade = 45;
		double altura = 1.74;
		
		System.out.println(nome);
		System.out.println(idade);
		System.out.println(altura);
		
	}
}
